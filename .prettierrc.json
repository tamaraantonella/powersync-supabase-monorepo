{
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "semi": true,
  "jsxSingleQuote": false,
  "useTabs": false,
  "quoteProps": "as-needed",
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"]
}
